                <span class="navbar-toggler-icon"></span>
            </button>

            <!-- Collapsible content -->
            <div class="collapse navbar-collapse" id="mainNavbar">

                <!-- Centered (Wishlist + Cart + Search) -->
                <ul class="navbar-nav mx-lg-auto mb-2 mb-lg-0 d-flex align-items-center justify-content-center fs-6 fs-lg-5">
                    <!-- Wishlist -->
                    <li class="nav-item mx-2">
                        <a asp-action="Index" asp-controller="Wishlists" class="nav-link d-flex align-items-center gap-1" aria-label="Wishlist">
                            <i class="bi bi-heart fs-5"></i> <span>Wishlist</span>
                            @if (User.Identity!.IsAuthenticated)
                            {
                                <span id="wishlist-count-id" class="wishlist-count ms-1">(0)</span>
                            }
                        </a>
                    </li>

                    <!-- Cart -->
                    <li class="nav-item mx-2 position-relative">
                        <a asp-action="Index" asp-controller="Cart" class="nav-link d-flex align-items-center gap-1" aria-label="Cart">
                            <i class="bi bi-cart fs-5"></i> <span>Cart</span>
                            @if (User.Identity!.IsAuthenticated)
                            {
                                <span id="cart-count"
                                      class="position-absolute top-0 start-100 translate-middle badge rounded-pill bg-danger">
                                    0
                                    <span class="visually-hidden">items in cart</span>
                                </span>
                            }
                        </a>
                    </li>

                    <!-- Search -->
                    <li class="nav-item mx-2">
                        <a href="#search" class="nav-link" aria-label="Search">
                            <svg width="24" height="24" viewBox="0 0 24 24"><use xlink:href="#search"></use></svg>
                        </a>
                    </li>
                </ul>

                <!-- Right side (Admin + Auth) -->
                <ul class="navbar-nav ms-auto mb-2 mb-lg-0 d-flex align-items-center">
                    <!-- Admin Panel -->
                    @if (User.Identity!.IsAuthenticated && User.IsInRole(UserRoles.Admin))
                    {
                        <li class="nav-item mx-2">
                            <a asp-controller="Product" asp-action="Index" class="btn btn-outline-primary text-uppercase">
                                Admin Panel
                            </a>
                        </li>
                    }

                    <!-- Auth Buttons -->
                    @if (User.Identity!.IsAuthenticated)
                    {
                        <li class="nav-item mx-2">
                            <form asp-action="Logout" asp-controller="Auths" method="post" class="d-inline">
                                @Html.AntiForgeryToken()
                                <button type="submit" class="btn btn-link text-uppercase p-0" style="text-decoration: none;">
                                    Logout
                                </button>
                            </form>
                        </li>
                        <li class="nav-item mx-2">
                            <span class="text-uppercase">@User.Identity.Name</span>
                        </li>
                    }
                    else
                    {
                        <li class="nav-item mx-2">
                            <a asp-action="Register" asp-controller="Auths" class="nav-link text-uppercase">Sign Up</a>
                        </li>
                        <li class="nav-item mx-2">
                            <a asp-action="Login" asp-controller="Auths" class="nav-link text-uppercase">Login</a>
                        </li>
                    }
                </ul>
            </div>
        </div>
    </nav>

